<%- include('../partials/header') %>

<form action="/cafes/<%= cafe.id %>?_method=PUT" method="POST">
  <input type="text" name="cafe" value="<%= cafe.cafe %>"><br>
  <label>
    <input type="checkbox" name="done" <%= cafe.done ? 'checked' : '' %>>
    Completed
  </label><br>
  <button type="submit">Update To-Do</button>
</form>

<%- include('../partials/footer') %>